  "quality": {
    "min_papers": 40,
    "min_review_score": 9.0,
    "max_review_iterations": 15,
    "max_response_rounds": 3,
    "dynamic_scoring": true,
    "required_sections": [
      "abstract",
      "introduction",
      "related_work",
      "methodology",
      "experiments",
      "results",
      "discussion",
      "conclusion"
    ],
    "section_min_word_count": {
      "introduction": 800,
      "related_work": 1500,
      "methodology": 1200,
      "experiments": 1000,
      "discussion": 800
    }
  },
  "paper": {
    "target_word_count": 10000,
    "default_venue": "AAAI"
  },
  "parallel": {
    "phase1_enabled": true,
    "use_task_tool": true,
    "timeout_per_agent": 300,
    "retry_on_failure": true,
    "max_retries": 1
  },
  "cache": {
    "enabled": true,
    "max_papers_per_domain": 500,
    "purge_after_days": 365,
    "auto_generate_index": true,
    "domains": {
      "multi_agent_systems": "MAS",
      "knowledge_graph": "KG",
      "nlp_to_sql": "NL2SQL",
      "bridge_engineering": "Bridge"
    }
  },
